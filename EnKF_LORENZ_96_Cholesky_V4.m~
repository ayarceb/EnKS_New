Error_ENKS(i)=norm(abs(sum(meanxa_EnKS(20,i)-Xreal(20,i))));hold on
set(gca, 'YScale', 'log')
end
plot(movmean(Error_ENKF,window))
plot(movmean(Error_ENKF_CHOLESKY,window))
plot(movmean(Error_ENKF_schur,window))
% plot(movmean(Error_ENKS,window))
%legend({'ENKF','ENKF MC','ENKF Schur','EnKS'})
legend({'ENKF','ENKF MC','ENKF Schur'})

