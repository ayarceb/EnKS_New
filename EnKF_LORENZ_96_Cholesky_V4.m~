% EnKF script for the Lorenz96 using th Modified Cholesky approximation of
% the B covariance matrix

clear all
close all
clc

%%  Parameters model, True generation   

rng(30);

Tsim=300;        % Simulation time
dt=0.01;         % Step length
m=40;            % Number of observations
n=40;            % State number
F=8;             % Forcing factor model
r=2;             % Predecesors radio
sigma=1.5;            % Noise factor for the error covariance matrix terms
R=sigma^2*eye(m);       % Observation Covariance matrix 
H = eye(n,n);           % Observation operator matrix
H = H(randperm(n,m),:);
pfactor = 1.2;  %INFLATION FACTOR
T_r_1 = localization_matrix(n,r);
                                                                                                                                                                                                                                                                                
%==============Generate real state=================
x0=1*rand(n,1);
[Xreal]=Lorenz_96(Tsim,dt,x0,F);            % Create vector


%==Number of Ensembles==
N=40 ;                  % Number of ensembles
Xb=zeros(n,N,Tsim);       % Background ensemble initialization
X_chol=zeros(n,N,Tsim);       % Background ensemble initialization
X_S=zeros(n,N,Tsim);

meanxb=zeros(n,Tsim);       % Background ensemble mean initialization
Xb(:,:,1)=0.5*rand(n,N);      % Initial condition creation

Xa=Xb;
X_chol=Xb;                   % Initialize the C                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 holesky scenario background matrix
X_schur=Xb;                  % Initialize the Schur scenario background matrix
X_S=Xb;


meanxa_EnKF=zeros(n,Tsim);              % initialization of the vector to put the an??lisis ENKF mean
meanxa_EnKF_CHOLESKY=zeros(n,Tsim);     % initialization of the vector to put the an??lisis ENKF_CHOLESKY mean
meanxa_EnKF_schur=zeros(n,Tsim);              % initialization of the vector to put the an??lisis ENKF mean
meanxa_EnKS=zeros(n,Tsim);              % initialization of the vector to put the analisis EnKS mean


B=zeros(n,n,Tsim);            % initialization of the covariance background error matrix for EnKF
BC=zeros(n,n,Tsim);           % initialization of the covariance background error matrix for Cholesky
BS=zeros(n,n,Tsim);            % initialization of the covariance background error matrix for Smoother

%%  Part for the observation frequency
% y=zeros(m,Tsim);
% tipo=7;
frequency=4; % Frequency of observations
muestreo=frequency:frequency:Tsim;     % vector to put the observation to assimilate
 
 Y=H*Xreal;     %  Sampling of the observation operator
 select=2; % this is used to select between the enkf (1), enkfMC (2), enks (3) and enksMC (4)

%% %==========Scenario EnKF ==========

% if select==1
%   
for i=1:Tsim-1
%======== Forecast Step==========

for en=1:N
     [Xb(:,en,i+1)]=Lorenz_96_one_step(1,dt,squeeze(Xb(:,en,i)),F);
end

     meanxb(:,i)=mean(Xb(:,:,i),2);    

if sum(muestreo==i)   %Validation if there are a observation available


%  Standard EnKF Background matrix 

%%%%INFLATION FACTOR

      Xbp(:,:,i)=meanxb(:,i)*ones(1,N)+ pfactor*(Xb(:,:,i)-meanxb(:,i)*ones(1,N));
      meanXbp(:,i) = mean(Xbp(:,:,i),2);   
%   for Nen=1:N 
%      XXb(:,Nen,i)= Xb(:,Nen,i)-meanxb(:,i);  
%   end
  i

       XXb(:,:,i) =Xbp(:,:,i)-meanXbp(:,i)*ones(1,N);


   B=((1/(N-1))*(XXb(:,:,i)*XXb(:,:,i)'));
%======================== Analysis Step ENKF=========================================
muestra=0;     
K=B*H'*pinv(H*B*H'+R);     % Matriz de Ganancia de Kalman

    for en=1:N

         C(:,i)= Y(:,i)+sigma*randn(m,1);        %  Almacenamiento de los datos sint??ticos
         Xa(:,en,i)=Xb(:,en,i)+K*(C(:,i)-H*Xb(:,en,i));

    end
    Xb(:,:,i+1)=Xa(:,:,i);
    meanxa_EnKF(:,i+1)=mean(Xb(:,:,i),2);
end
if sum(muestreo~=i) 
i
  meanxa_EnKF(:,i+1)=mean(Xb(:,:,i),2);
end

 
%end

%% %==========Scenario EnKF_Schur_Localization ==========

% if select==1
%   
 for i=1:Tsim-1
% %======== Forecast Step==========
for en=1:N
     [X_schur(:,en,i+1)]=Lorenz_96_one_step(1,dt,squeeze(X_schur(:,en,i)),F);
  end
  
     meanxb(:,i)=mean(X_schur(:,:,i),2);     
%      
  if sum(muestreo==i)   %Validation if there are a observation available
%      
%%%%INFLATION FACTOR 
      Xbp(:,:,i)=meanxb(:,i)*ones(1,N)+ pfactor*(X_schur(:,:,i)-meanxb(:,i)*ones(1,N));
      meanXbp(:,i) = mean(Xbp(:,:,i),2);   
%   for Nen=1:N 
%      XXb(:,Nen,i)= X_schur(:,Nen,i)-meanxb(:,i);  
%   end
  i
  
       XXb(:,:,i) =Xbp(:,:,i)-meanXbp(:,i)*ones(1,N);
%   
% %  Standard EnKF Background matrix 
% 
    B=((1/(N-1))*(XXb(:,:,i)*XXb(:,:,i)'));
%     B=T_r_1.*B; 
% %======================== Analysis Step ENKF=========================================

muestra=0;     
K=B*H'*pinv(H*B*H'+R);     % Matriz de Ganancia de Kalman
  
    for en=1:N
         
         C(:,i)= Y(:,i)+sigma*randn(m,1);        %  Almacenamiento de los datos sint??ticos
         Xa(:,en,i)=X_schur(:,en,i)+K*(C(:,i)-H*X_schur(:,en,i));

    end
    X_schur(:,:,i+1)=Xa(:,:,i);
    meanxa_EnKF_schur(:,i+1)=mean(X_schur(:,:,i),2);
end
if sum(muestreo~=i) 
i
  meanxa_EnKF_schur(:,i+1)=mean(X_schur(:,:,i),2);
end


% end
end

%% %=================Scenario EnKF CHOLESKY ==========

%if select==2
  
for i=1:Tsim-1
%================= Forecast Step==========
   for en=1:N
     [X_chol(:,en,i+1)]=Lorenz_96_one_step(1,dt,squeeze(X_chol(:,en,i)),F);
   end
   

     meanxb(:,i)=mean(X_chol(:,:,i),2);    

%      
%%%%INFLATION FACTOR 
      Xbp(:,:,i)=meanxb(:,i)*ones(1,N)+ pfactor*(X_chol(:,:,i)-meanxb(:,i)*ones(1,N));
      meanXbp(:,i) = mean(Xbp(:,:,i),2);   
%   for Nen=1:N 
%      XXb(:,Nen,i)= X_chol(:,Nen,i)-meanxb(:,i);  
%   end
  i
  
       XXb(:,:,i) =Xbp(:,:,i)-meanXbp(:,i)*ones(1,N);
%   i
%    %XXa=XXb(:,:,i)*sqrt(1/(N-1));   
    BC(:,:,i)=Calculo_B_Cholesky(XXb,r); %Estimation of Covariance by Modified Cholesky
% %  inB=Calculo_inB_Cholesky(XXa,r); %Estimation of invserse Covariance by Modified Cholesky
% %  Bsquare=B^(1/2);
% %  L=Xb(:,:,i)-meanxb;
% %     
% %%  Standard EnKF MC Background matrix 
% 
%   
%      
% %=============== Analysis Step ENKF MC========================================     
if sum(muestreo==i)   %Validation if there are a observation available    
KC=BC(:,:,i)*H'*pinv(H*BC(:,:,i)*H'+R);     % Matriz de Ganancia de Kalman para cholesky
%      
%      
      for en=1:N
%          
          C(:,i)= Y(:,i)+sigma*randn(m,1);        %  Almacenamiento de los datos sint??ticos
          Xa_C(:,en,i)=X_chol(:,en,i)+KC*(C(:,i)-H*X_chol(:,en,i));
% 
      end
%      
      X_chol(:,:,i+1)=Xa_C(:,:,i);
    
  meanxa_EnKF_CHOLESKY(:,i)=mean(X_chol(:,:,i+1),2);
%      
end                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
 if sum(muestreo~=i) 
 
  meanxa_EnKF_CHOLESKY(:,i)=mean(X_chol(:,:,i+1),2);
 end

end
% for i=1:Tsim
% imagesc(BC(:,:,i))
% colorbar
% title(sprintf('Tiempo %i',i))
% pause(2)
% end

%% %=================Scenario EnKS ==================
% % if select==1
% %   
% for i=1:Tsim-1
% %======== Forecast Step==========
% 
% for en=1:N
%      [X_S(:,en,i+1)]=Lorenz_96_one_step(1,dt,squeeze(X_S(:,en,i)),F);
%   end
%   
%      meanxb(:,i)=mean(X_S(:,:,i),2);    
%     
%   for Nen=1:N
%      XXb(:,Nen,i)= X_S(:,Nen,i)-meanxb(:,i);  
%   end
%   i
%   
% %  Standard EnKF Background matrix 
% 
%    B(:,:,i)=((1/N-1)*(XXb(:,:,i)*XXb(:,:,i)'));
% 
%    if sum(muestreo==i)   %Validation if there are a observation available
% %======================== Analysis Step ENKF=========================================
% 
% muestra=0;     
% K(:,:,i)=B(:,:,i)*H'*pinv(H*B(:,:,i)*H'+R);     % Matriz de Ganancia de Kalman
% 
%     for en=1:N
%          
%          C(:,i)= Y(:,i)+sigma*randn(m,1);        %  Almacenamiento de los datos sint??ticos
%          Xa(:,en,i)=X_S(:,en,i+1)+(K(:,:,i)*(C(:,i)-H*X_S(:,en,i))+K(:,:,i-1)*(C(:,i-1)-H*X_S(:,en,i-1))+K(:,:,i-2)*(C(:,i-2)-H*X_S(:,en,i-2)))/40;
% % for i=1:Tsim
% % imagesc(B(:,:,i))
% % colorbar
% % title(sprintf('Tiempo %i',i))
% % pause(2)
% % end
% 
%     end
%     X_S(:,:,i+1)=Xa(:,:,i);
%     meanxa_EnKS(:,i+1)=mean(X_S(:,:,i),2);
% end
% if sum(muestreo~=i) 
% i
%   meanxa_EnKS(:,i+1)=mean(X_S(:,:,i),2);
% end
%                                                                                                                                                                                                                                                                                                                                                 
% end
%end   
     
% for i=1:Tsim
% imagesc(B(:,:,i))
% colorbar
% title(sprintf('Tiempo %i',i))
% pause(2)
% end

                                                                                                                                                                                                                                                                                                

%% just to plot the simulation ensemble and the mean
window=10;  %movil mean

for i=1:N
plot(squeeze(Xb(1,i,:)))
    hold on
end
plot(meanxb(1,:),'k','LineWidth',3)
figure

% imagesc(B);
% colormap(flipud(hot));colorbar;title('r = 1');xlabel('state number');ylabel('state number')
% caxis([0 0.5]);
% figure
% imagesc(P0);
% colormap(flipud(hot));colorbar;title('r = 1');xlabel('state number');ylabel('state number')
% caxis([0 0.5]);



error_EnKF=norm(abs(sum(meanxa_EnKF(:,:)-Xreal(:,:))))
figure
imagesc(Xreal),title('Truth State')
figure
